{
  "name": "orchestrator-relay",
  "version": "2.0.10",
  "description": "Agent-to-agent coordination relay for autonomous task execution",
  "main": "dist/cli.js",
  "bin": {
    "relay": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/shell/mcp.ts",
    "start": "node dist/shell/mcp.js",
    "inspector": "npx @modelcontextprotocol/inspector node dist/shell/mcp.js",
    "prepublishOnly": "npm run build",
    "test": "npx tsx --test 'src/**/*.test.ts'"
  },
  "files": [
    "dist/",
    "bin/",
    "prompts/",
    "templates/",
    "README.md"
  ],
  "keywords": [
    "relay",
    "agent",
    "coordination",
    "llm",
    "cli"
  ],
  "author": "tsomaia",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@types/nunjucks": "^3.2.6",
    "chalk": "^4.1.2",
    "commander": "^14.0.3",
    "fs-extra": "^11.3.3",
    "inquirer": "^8.2.6",
    "nunjucks": "^3.2.4",
    "proper-lockfile": "^4.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@types/proper-lockfile": "^4.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
